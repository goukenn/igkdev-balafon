$t->addObData(function(){
    $o="";
    foreach(get_defined_functions()["user"] as $f){
        if(preg_match("/^igk_io/", $f)){
            try {
                $v=new ReflectionFunction($f);
                if($v->getNumberOfRequiredParameters()<=0)
                    $o .= $f. " = ".$f()." <br />";
            }
            catch(Exception $ex){}
        }
    }
    igk_wln($o);
});